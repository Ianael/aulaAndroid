<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ianael.sistema_area">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DadosCirculo"
            android:label="@string/title_activity_dados_circulo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DadosTriangulo"
            android:label="@string/title_activity_dados_triangulo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DadosRetangulo"
            android:label="@string/title_activity_dados_retangulo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Area"
            android:label="@string/title_activity_area"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>